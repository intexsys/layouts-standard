{
    "name": "netgen/layouts-standard",
    "description": "Netgen Layouts is an easy and flexible way of managing all page layouts on your website. This package provides a set of most used blocks and layouts for Netgen Layouts for building rich content websites.",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://www.netgenlabs.com"
        }
    ],
    "require": {
        "netgen/layouts-core": "~1.0.0",
        "erusev/parsedown": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/var-dumper": "^3.4 || ^4.3",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "franzl/studio": "*"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "coverage": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --coverage-html=coverage --colors=always",
        "phpstan": "@php vendor/bin/phpstan analyse -c phpstan.neon --level=7 --ansi lib bundle",
        "phpstan-tests": "@php vendor/bin/phpstan analyse -c phpstan.tests.neon --level=7 --ansi tests"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\Layouts\\Standard\\": "lib/",
            "Netgen\\Bundle\\LayoutsStandardBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\Layouts\\Standard\\Tests\\": "tests/lib/",
            "Netgen\\Layouts\\Tests\\": "vendor/netgen/layouts-core/tests/lib/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
